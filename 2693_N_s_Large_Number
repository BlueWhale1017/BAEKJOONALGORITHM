#include<iostream>
#include<vector>
#include<algorithm>

using namespace std;

int main(void)
{
	int T = 0;
	int arr[10] = { 0, };
	int max = 0;

	int i = 0, j = 0;

	cin >> T;

	for (j = 0; j < T; j++)
	{
		for (i = 0; i < 10; i++)
		{
			cin >> arr[i];
		}
		sort(arr, arr + 10);
		cout << arr[7] << endl;
	}
	return 0;
}
